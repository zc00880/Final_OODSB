<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="400.0" prefWidth="610.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.MainItemsController">
   <center>
      <Pane prefHeight="400.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <DatePicker layoutX="7.0" layoutY="10.0" prefHeight="35.0" prefWidth="170.0" />
            <VBox layoutX="188.0" layoutY="10.0" prefHeight="350.0" prefWidth="200.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Job Pool:" textAlignment="CENTER" wrappingWidth="200.0">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Text>
                  <ListView fx:id="jobList" onMouseClicked="#displaySelected" prefHeight="290.0" prefWidth="175.0" />
                  <HBox prefHeight="39.0" prefWidth="175.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#goAddJob" prefHeight="35.0" prefWidth="78.0" text="Add" />
                        <Button mnemonicParsing="false" onAction="#goEditJob" prefHeight="35.0" prefWidth="78.0" text="Edit" />
                        <Button mnemonicParsing="false" onAction="#deleteJob" prefHeight="35.0" prefWidth="78.0" text="Delete" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox layoutX="395.0" layoutY="10.0" prefHeight="350.0" prefWidth="200.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Resource Pool:" textAlignment="CENTER" wrappingWidth="200.0">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Text>
                  <ListView prefHeight="290.0" prefWidth="175.0" />
                  <HBox prefHeight="39.0" prefWidth="175.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#goAddResource" prefHeight="35.0" prefWidth="78.0" text="Add" />
                        <Button mnemonicParsing="false" prefHeight="35.0" prefWidth="78.0" text="Edit" />
                        <Button mnemonicParsing="false" prefHeight="35.0" prefWidth="78.0" text="Delete" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <TextField fx:id="textBox" editable="false" layoutX="18.0" layoutY="173.0" />
         </children>
      </Pane>
   </center>
</BorderPane>
